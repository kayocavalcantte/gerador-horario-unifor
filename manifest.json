{
  "name": "Unifor Timetable Generator",
  "description": "Captura dados de disciplinas e gera grade de hor√°rios automaticamente.",
  "author": "Kayo Duarte Cavalcante",
  "homepage_url": "https://github.com/kayocavalcantte/gerador-horario-unifor",
  "version": "1.1",
  "manifest_version": 3,
  "permissions": ["storage"],
  "host_permissions": [
    "https://uol.unifor.br/*"
  ],
  "web_accessible_resources": [
    {
      "resources": ["javascript/inject.js"],
      "matches": ["https://uol.unifor.br/*"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["https://uol.unifor.br/*"],
      "js": ["javascript/content.js"],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_popup": "hello.html",
    "default_icon": "./assets/icon-logo.png"
  }
}